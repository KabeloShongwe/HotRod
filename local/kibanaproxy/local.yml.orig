SIGNING_SECRET: {{ userauth.secret }}

# Proxy Settings
PROXY_TARGET: http://kibana:5601
# Optional whitelist
PROXY_TARGET_WHITELIST:
  - ANY ^(?!\/elasticsearch).*$
  - HEAD ^/elasticsearch/$
  - GET ^/elasticsearch/_nodes$
  - ANY ^/elasticsearch/kibana-int.*$
  - ANY ^/elasticsearch/logstash.*$
  - ANY ^/elasticsearch/_mget\?preference=.*$
  - ANY ^/elasticsearch/_msearch\?preference=.*$
JWT_SECURED_ROUTE: /elasticsearch

# IFrame Settings
IFRAME_APP_ROUTE: /kibana
IFRAME_SOURCE: {{ addresses.kibana }}
IFRAME_ADD_JWT_REGEX: /elasticsearch/.*$

# Auth URL
AUTH_URL: {{ userauth.url }}
